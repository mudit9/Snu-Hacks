<?xml version="1.0" encoding="utf-8"?>


<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fancy="http://schemas.android.com/apk/res-auto"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:maxLines = "10000"
    android:scrollbars = "vertical"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:background="@drawable/backgrounddown"
    tools:context=".messMenu">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MESS MENU"
        android:id="@+id/welcome7"
        android:textStyle="bold"
        android:textSize="50sp"
        android:layout_marginTop="14dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textAlignment="center"
        android:textColor="@color/white" />

    <com.wang.avi.AVLoadingIndicatorView
        android:id="@+id/avielement2"
        style="@style/AVLoadingIndicatorView"
        app:indicatorName="LineScalePulseOutRapidIndicator"
        app:indicatorColor="@color/white"
        android:layout_height="75dp"
        android:visibility="gone"
        android:layout_below="@+id/dh22"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:layout_width="75dp" />

    <com.wang.avi.AVLoadingIndicatorView
        android:id="@+id/avielement3"
        style="@style/AVLoadingIndicatorView"
        app:indicatorName="BallClipRotateMultipleIndicator"
        app:indicatorColor="@color/white"
        android:layout_width="75dp"
        android:layout_below="@+id/dh22"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="70dp"
        android:visibility="gone"
        android:layout_height="75dp" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:layout_width="wrap_content"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        android:paddingTop="10dp"
        fancy:fb_text="DH2"
        fancy:fb_textColor="@color/white"
        fancy:fb_defaultColor="@color/black"
        fancy:fb_focusColor="#B5BFC0"
        fancy:fb_borderWidth="3dp"
        fancy:fb_borderColor="@color/realwhite"
        fancy:fb_radius="50dp"
        android:layout_height="wrap_content"
        android:id="@+id/dh22"
        android:textColor="@color/white"
        android:layout_marginRight="35dp"
        android:layout_marginEnd="35dp"
        android:layout_alignTop="@+id/dh11"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="500dp"
        android:id="@+id/menu1"
        android:textSize="18sp"
        android:layout_height="275dp"
        android:layout_below="@+id/dh11"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp"
        android:layout_alignParentBottom="true"
        android:textColor="@color/white"
        android:textAlignment="center" />

    <mehdi.sakout.fancybuttons.FancyButton
        android:layout_width="wrap_content"
        android:paddingBottom="10dp"
        android:paddingLeft="20dp"
        android:paddingRight="20dp"
        fancy:fb_textColor="@color/white"
        fancy:fb_defaultColor="@color/black"
        fancy:fb_focusColor="#B5BFC0"
        fancy:fb_borderWidth="3dp"
        fancy:fb_borderColor="@color/realwhite"
        fancy:fb_radius="50dp"
        android:paddingTop="10dp"
        fancy:fb_text="DH1"
        android:layout_height="wrap_content"
        android:id="@+id/dh11"
        android:textColor="@color/white"
        android:layout_marginTop="26dp"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp"
        android:layout_below="@+id/welcome7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

</RelativeLayout>